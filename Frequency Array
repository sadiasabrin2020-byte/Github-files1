#include<iostream>
using namespace std;
int main()
{
    int N,M;
    cin>>N>>M;
    int arr[N];
    for(int i=0; i<N; i++)
    {
        cin>>arr[i];
    }

    int freq[M+1]={0};
    for(int i=0;i<N;i++){
        freq[arr[i]]++;
    }

    for(int i=1;i<M;i++){
        cout<<freq[i]<<endl;
    }

    return 0;
}

